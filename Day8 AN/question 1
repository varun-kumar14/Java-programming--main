// 1. Calculator class with method overloading
public class Calculator {
    public int multiply(int num1, int num2) {
        return num1 * num2;
    }

    public double multiply(double num1, double num2, double num3) {
        return num1 * num2 * num3;
    }

    public static void main(String[] args) {
        Calculator calculator = new Calculator();
        
        // Using the first version of multiply method
        int result1 = calculator.multiply(5, 7);
        System.out.println("Result 1: " + result1);

        // Using the second version of multiply method
        double result2 = calculator.multiply(2.5, 3.0, 1.5);
        System.out.println("Result 2: " + result2);
    }
}
